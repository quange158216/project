<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
 <struts>

 	<package name="users" namespace="/users" extends="common">
 		<action name="*" class="usersAction" method="{1}">
 			<result name="login" >/views/login.jsp</result>
 			<result name="index" type="redirect">/main.jsp</result>
 			<result name="success" >/views/success.jsp</result>
 			<result name="reg" >/views/reg.jsp</result>
 			
 		</action>
		
 	</package>
 </struts>